{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "imports": {
    "@std/": "https://deno.land/std@0.156.0/",
    "@shared/": "./src/@shared/",
    "@vanilla-jsx/": "./server/@vanilla-jsx/"
  },
  "tasks": {
    "start": "deno run -A --unstable https://deno.land/x/denon/denon.ts run -A ./server/mod.tsx"
  }
}
